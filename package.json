{
  "name": "rkyv-js",
  "version": "0.1.0",
  "description": "A TypeScript decoder library for rkyv (Rust zero-copy deserialization framework)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schema": {
      "import": "./dist/schema.js",
      "types": "./dist/schema.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "benchmark": "node --experimental-transform-types benchmarks/index.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "rkyv",
    "rust",
    "serialization",
    "deserialization",
    "zero-copy",
    "binary"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "capnp-es": "^0.0.14",
    "mitata": "^1.0.34",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "cbor-x": "^1.6.0",
    "protobufjs": "^8.0.0"
  }
}
